Activities = '[{"title" : "Portion sizes", "image" : "./img/nutBalancedMeal.svg", "shortContent" : "Be mindful of portion. Ideally six small meals each day", "extendedContent" : "You’ve probably heard the advice that eating small meals throughout the day is how you win the battle of the bulge. The claim is that frequent snacking, as long as it’s healthy, keeps your metabolism humming, staves off hunger, and controls blood sugar."},{"title" : "Eat breakfast", "image" : "./img/nutBreakfast.svg", "shortContent" : "Eat breakfast everyday, within an hour of getting up in the morning.", "extendedContent" : "1. Starting your morning with breakfast will boost your metabolism and keep your blood sugar levels stable during the day. 2. A morning meal helps energize your body, keeping you active and productive until lunch."},{"title" : "Eat balancd meals", "image" : "./img/nutHealthyFats.svg", "shortContent" : "Having a balanced meal with equal amount of carbs and protein can help you to fell fuller and have more energy.", "extendedContent" : ""},{"title" : "Eat healthy fats", "image" : "./img/nutHealthyFats.svg", "shortContent" : "Healthy fats like avocado, nuts, and animal fats are delicious and good for you.", "extendedContent" : ""},{"title" : "Drink Water", "image" : "./img/nutWater.svg", "shortContent" : "2-3 liters of water each day can make you feel better", "extendedContent" : ""},{"title" : "Be prepared!", "image" : "./img/nutForkandKnife.svg", "shortContent" : "Try to anticipate what food you will need and carry a cooler pack with clean foods each day", "extendedContent" : ""},{"title" : "Eat fruits & veggies", "image" : "./img/nutFruits.svg", "shortContent" : "Depend on fresh & vegetables for fiber, viamins, nutrients & enzymes.", "extendedContent" : ""}]'